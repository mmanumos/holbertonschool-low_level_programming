#include "holberton.h"
#include <stdio.h>
/**
 * _strcpy - reset values from from pointer
 * @dest: pointed array
 * @src: String to copy
 *
 * Return - Return string to pointer
 */
char *_strcpy(char *dest, char *src)
{
	int cont = 0;

	while (src[cont] != '\0')
	{
		dest[cont] = src[cont];
		cont++;
	}
	dest[cont] = '\0';
	return (dest);


}
