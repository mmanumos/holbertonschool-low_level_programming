#include <stdio.h>

/**
 * main - Prints alphabet
 * letter: letter
 *
 * Description: Prints lowercase and uppercase alphabet
 * Return: Always 0
 */


int main(void)
{
	char letter;

	for (letter = '0'; letter <= '9'; letter++)
	{
		putchar(letter);
	}
	for (letter = 'a'; letter <= 'f'; letter++)
	{
		putchar(letter);
		if (letter == 'f')
		{
			putchar('\n');
		}
	}
	return (0);
}
